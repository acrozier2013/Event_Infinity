<table id="searchresults" style="width:100%">
    <tr>
        <p></p>
        <th>Title</th>
        <th>Start Date</th>
        <th>Location</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a href="~/Events/Details\@item.EventId">@item.EventTitle</a>
            </td>
            <td>
                @item.EventStartDate
            </td>
            <td>
                @item.EventLocation
            </td>
        </tr>
    }
</table>

@if (Model.Count() == 0)
{
    <div>
        <p>Hmm... looks like there are no results for your search.</p>
    </div>

}